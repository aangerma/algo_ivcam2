<?xml version="1.0" encoding="utf-8"?>
<CustomRegisters Name="Custom" DeviceType="DS5" ASIC="B0">
	<RegisterGroup Name="Depth Controls">
		<Register Name="DeepSea_Robbins_Munroe" DisplayName="Robbins Monro">
			<RegisterField Name="Plus Increment" DisplayName="Estimate Median Increment" Type="Decimal">
				<Range MinValue="0" MaxValue="255" Step="1"/>
			</RegisterField>
			<RegisterField Name="Minus Decrement" DisplayName="Estimate Median Decrement" Type="Decimal">
				<Range MinValue="0" MaxValue="255" Step="1"/>
			</RegisterField>
		</Register>
		<Register Name="DeepSea_Median_Threshold">
			<RegisterField Name="DeepSea Median Threshold" DisplayName="Median Threshold" Type="Decimal">
				<Range MinValue="0" MaxValue="1023" Step="1"/>
			</RegisterField>
		</Register>
		<Register Name="DeepSea_Score_Threshold">
			<RegisterField Name="Score Thresh A" DisplayName="Score Minimum Threshold" Type="Decimal">
				<Range MinValue="0" MaxValue="1023" Step="1"/>
			</RegisterField>
			<RegisterField Name="Score Thresh B" DisplayName="Score Maximum Threshold" Type="Decimal">
				<Range MinValue="0" MaxValue="4095" Step="1"/>
			</RegisterField>
		</Register>
		<Register Name="DeepSea_Texture_Threshold">
			<RegisterField Name="Texture Difference Threshold" DisplayName="Texture Difference Threshold" Type="Decimal">
				<Range MinValue="0" MaxValue="1023" Step="1"/>
			</RegisterField>
			<RegisterField Name="Texture Count Threshold" DisplayName="Texture Count Threshold" Type="Decimal">
				<Range MinValue="0" MaxValue="31" Step="1"/>
			</RegisterField>
		</Register>
		<Register Name="DeepSea_Second_Peak_Threshold">
			<RegisterField Name="DeepSea Second Peak Threshold" DisplayName="DeepSea Second Peak Threshold" Type="Decimal">
				<Range MinValue="0" MaxValue="1023" Step="1"/>
			</RegisterField>
		</Register>
		<Register Name="DeepSea_Neighbor_Threshold">
			<RegisterField Name="DeepSea Neighbor Threshold" DisplayName="DeepSea Neighbor Threshold" Type="Decimal">
				<Range MinValue="0" MaxValue="1023" Step="1"/>
			</RegisterField>
		</Register>
		<Register Name="DeepSea_LR_Threshold">
			<RegisterField Name="LR Agree Threshold" DisplayName="LR Threshold" Type="Decimal">
				<Range MinValue="0" MaxValue="2047" Step="1"/>
			</RegisterField>
		</Register>
	</RegisterGroup>
	<RegisterGroup Name="RSM">
		<Register Name="RSM_control_Reg">
			<RegisterField Name="RSM_Bypass" DisplayName="RSM Bypass" Type="Bool">
				<Boolean/>
			</RegisterField>
			<RegisterField Name="diff_thresh" DisplayName="Disparity Diff Threshold" Type="Float">
				<Range MinValue="0" MaxValue="7.96875" Step="0.03125" Converter="FixedPointFloat(3.5)"/>
			</RegisterField>
			<RegisterField Name="slo_rau_diff_thresh" DisplayName="SLO RAU Diff Threshold" Type="Float">
				<Range MinValue="0" MaxValue="3.96875" Step="0.03125" Converter="FixedPointFloat(2.5)"/>
			</RegisterField>
			<RegisterField Name="remove_thresh" DisplayName="Remove Threshold" Type="Decimal">
				<Range MinValue="0" MaxValue="168" Step="1"/>
			</RegisterField>
		</Register>
	</RegisterGroup>
	<RegisterGroup Name="RAU Support Vector Control">
		<Register Name="RAU_Support_Vector_Minima">
			<RegisterField Name="minWest" DisplayName="Min West" Type="Decimal">
				<Range MinValue="0" MaxValue="7" Step="1"/>
			</RegisterField>
			<RegisterField Name="minEast" DisplayName="Min East" Type="Decimal">
				<Range MinValue="0" MaxValue="7" Step="1"/>
			</RegisterField>
			<RegisterField Name="minWEsum" DisplayName="Min WE Sum" Type="Decimal">
				<Range MinValue="0" MaxValue="15" Step="1"/>
			</RegisterField>
			<RegisterField Name="minNorth" DisplayName="Min North" Type="Decimal">
				<Range MinValue="0" MaxValue="3" Step="1"/>
			</RegisterField>
			<RegisterField Name="minSouth" DisplayName="Min South" Type="Decimal">
				<Range MinValue="0" MaxValue="3" Step="1"/>
			</RegisterField>
			<RegisterField Name="minNSsum" DisplayName="Min NS Sum" Type="Decimal">
				<Range MinValue="0" MaxValue="7" Step="1"/>
			</RegisterField>
			<RegisterField Name="uShrink" DisplayName="U Shrink" Type="Decimal">
				<Range MinValue="0" MaxValue="4" Step="1"/>
			</RegisterField>
			<RegisterField Name="vShrink" DisplayName="V Shrink" Type="Decimal">
				<Range MinValue="0" MaxValue="4" Step="1"/>
			</RegisterField>
		</Register>
	</RegisterGroup>
	<RegisterGroup Name="Color Control">
		<Register Name="Color_Control">
			<RegisterField Name="disableSADColor" DisplayName="Disable SAD Color" Type="Bool">
				<Boolean/>
			</RegisterField>
			<RegisterField Name="disableRAUColor" DisplayName="Disable RAU Color" Type="Bool">
				<Boolean/>
			</RegisterField>
			<RegisterField Name="disableSLORightColor" DisplayName="Disable SLO Right Color" Type="Bool">
				<Boolean/>
			</RegisterField>
			<RegisterField Name="disableSLOLeftColor" DisplayName="Disable SLO Left Color" Type="Bool">
				<Boolean/>
			</RegisterField>
			<RegisterField Name="disableSADNormalize" DisplayName="Disable AD Normalize" Type="Bool">
				<Boolean/>
			</RegisterField>
		</Register>
	</RegisterGroup>
	<RegisterGroup Name="RAU Color Thresholds Control">
		<Register Name="RAU_Threshold">
			<RegisterField Name="RAUDiffThresholdRed" DisplayName="Diff Threshold Red" Type="Decimal">
				<Range MinValue="1" MaxValue="1022" Step="1"/>
			</RegisterField>
			<RegisterField Name="RAUDiffThresholdGreen" DisplayName="Diff Threshold Green" Type="Decimal">
				<Range MinValue="1" MaxValue="1022" Step="1"/>
			</RegisterField>
			<RegisterField Name="RAUDiffThresholdBlue" DisplayName="Diff Threshold Blue" Type="Decimal">
				<Range MinValue="1" MaxValue="1022" Step="1"/>
			</RegisterField>
		</Register>
	</RegisterGroup>
	<RegisterGroup Name="SLO Color Thresholds Control">
		<Register Name="SLO_Threshold">
			<RegisterField Name="DiffThresholdRed" DisplayName="Diff Threshold Red" Type="Decimal">
				<Range MinValue="1" MaxValue="1022" Step="1"/>
			</RegisterField>
			<RegisterField Name="DiffThresholdGreen" DisplayName="Diff Threshold Green" Type="Decimal">
				<Range MinValue="1" MaxValue="1022" Step="1"/>
			</RegisterField>
			<RegisterField Name="DiffThresholdBlue" DisplayName="Diff Threshold Blue" Type="Decimal">
				<Range MinValue="1" MaxValue="1022" Step="1"/>
			</RegisterField>
		</Register>
	</RegisterGroup>
	<RegisterGroup Name="SLO Penalty Control">
		<Register Name="SLO_K1Penalty" DisplayName="Single Disparity Penalty" Type="Decimal">
			<Range MinValue="1" MaxValue="511" Step="1"/>
		</Register>
		<Register Name="SLO_K2Penalty" DisplayName="Multi Disparity Penalty" Type="Decimal">
			<Range MinValue="1" MaxValue="511" Step="1"/>
		</Register>
		<Register Name="SLO_K1PenaltyMod1" DisplayName="Single Disparity One Discon Penalty" Type="Decimal">
			<Range MinValue="1" MaxValue="511" Step="1"/>
		</Register>
		<Register Name="SLO_K2PenaltyMod1" DisplayName="Multi Disparity One Discon Penalty" Type="Decimal">
			<Range MinValue="1" MaxValue="511" Step="1"/>
		</Register>
		<Register Name="SLO_K1PenaltyMod2" DisplayName="Single Disparity Two Discon Penalty" Type="Decimal">
			<Range MinValue="1" MaxValue="511" Step="1"/>
		</Register>
		<Register Name="SLO_K2PenaltyMod2" DisplayName="Multi Disparity Two Discon Penalty" Type="Decimal">
			<Range MinValue="1" MaxValue="511" Step="1"/>
		</Register>
	</RegisterGroup>
	<RegisterGroup Name="HDAD">
		<Component Name="HDAD"/>
	</RegisterGroup>
</CustomRegisters>